{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "pnpm turbo build --filter=dapp-template-evm --force",
    "outputDirectory": "apps/dapp-template-evm/out",
    "installCommand": "pnpm install",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}